<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RollViewPager">
        <attr name="banner_hint_mode">
            <enum name="point" value="0" />
            <enum name="number" value="1" />
        </attr>
        <attr name="banner_hint_gravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
        <attr name="banner_hint_paddingRight" format="dimension" />
        <attr name="banner_hint_paddingLeft" format="dimension" />
        <attr name="banner_hint_paddingTop" format="dimension" />
        <attr name="banner_hint_paddingBottom" format="dimension" />
        <attr name="banner_play_delay" format="integer" />
        <attr name="banner_hint_color" format="color" />
        <attr name="banner_hint_alpha" format="integer" />
    </declare-styleable>

    <item name="viewpager_inner" type="id" />
</resources>